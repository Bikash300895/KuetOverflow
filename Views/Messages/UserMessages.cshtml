@model List<KuetOverflow.Models.Message>

@{
    ViewBag.Title = "Messages";
    Layout = "_StudentLayout";
}



<div class="row">
    <div class="col-md-2">
        <h2>Users</h2>
    </div>
    <div class="col-md-8">
        <h2>Messages</h2>
        @foreach (var message in Model)
        {
            <div>@message.Body</div>
        }
    </div>
    <div class="col-md-2">
        <h2>Following</h2>
    </div>
</div>
